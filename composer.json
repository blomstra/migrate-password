{
    "name": "blomstra/migrate-password",
    "description": "Migrates imported, old passwords to use the fill the Flarum password field.",
    "keywords": [
        "extension",
        "flarum",
        "password",
        "migrate"
    ],
    "license": "mit",
    "type": "flarum-extension",
    "authors": [
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "https://luceos.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "flarum/core": "^1.8.0"
    },
    "autoload": {
        "psr-4": {
            "Blomstra\\MigratePassword\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1",
        "phpstan/phpstan": "1.11.*"
    },
    "extra": {
        "flarum-extension": {
            "title": "Migrate Password",
            "category": "feature",
            "icon": {
                "image": "resources/assets/logo.png",
                "backgroundColor": "#E3E7F1",
                "backgroundSize": "60%",
                "backgroundRepeat": "no-repeat",
